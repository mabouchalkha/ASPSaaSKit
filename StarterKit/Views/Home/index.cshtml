<!DOCTYPE html>
<html ng-controller="appController as vm">
<head>
    <title ng-bind="vm.title"></title>
        @Styles.Render("~/Content/css")
</head>
<body id="app" data-custom-background data-toggle-off-canvas style="padding-top: 70px;">
    <div id="overlay">
        <svg fill="#000" width="140" height="64" xmlns="http://www.w3.org/2000/svg">
            <img id="loading" src="/Content/Images/grid.svg" />
        </svg>
    </div>
    <div>
        <div id="navigation_container" data-ng-cloak class="no-print">
            <section data-ng-include="'Scripts/Pages/header.html'" id="header" class="top-header"></section>
        </div>
        <div id="page-container row" class="container-fluid">
            <div id="nav-container"><aside class="col-md-2" data-ng-include="'Scripts/Pages/nav.html'"></aside></div>
            <div id="content" class="col-md-10"><section data-ui-view></section></div>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.15/angular-ui-router.min.js"></script>
    <script src="https://code.angularjs.org/1.3.15/angular-resource.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/0.12.1/ui-bootstrap-tpls.min.js"></script>
    @Scripts.Render("~/bundles/app")
</body>
</html>